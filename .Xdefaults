!!! xrdb -load ~/.Xdefaults && rxvt

!!! for rxvt under cygwin, so don't mind if xrdb -load failes under linux
Rxvt*font: "Consolas-21"
Rxvt*boldFont: "Consolas-21"
Rxvt*foreground: #bebebe
Rxvt*background: #000000

!!! for xterm under unix
XTerm*faceName: "DejaVu Sans Mon"
XTerm*faceSize: 16
XTerm*boldMode: false
XTerm*boldFont:
XTerm*metaSendsEscape: true
XTerm*eightBitInput: false

*foreground: #bebebe
*background: #000000

!black
*color0:		#000000
*color8:		#666666
!red
*color1:		#9e1828
*color9:		#cf6171
!green
